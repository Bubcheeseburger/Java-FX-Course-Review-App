<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="200.0" prefWidth="280.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="reviews.LogInController">
   <children>
      <Label alignment="CENTER_RIGHT" layoutX="30.0" layoutY="56.0" text="Username" textAlignment="RIGHT" />
      <Label alignment="CENTER_RIGHT" layoutX="30.0" layoutY="92.0" prefHeight="17.0" prefWidth="54.0" text="Password" textAlignment="RIGHT" />
      <TextField fx:id="username" layoutX="93.0" layoutY="52.0" promptText="Enter Username" />
      <PasswordField fx:id="password" layoutX="93.0" layoutY="88.0" promptText="Enter Password" />
      <Button layoutX="26.0" layoutY="145.0" mnemonicParsing="false" onAction="#handleLogInButton" text="Log In" />
      <Button layoutX="92.0" layoutY="145.0" mnemonicParsing="false" onAction="#handleCreateAccountButton" text="Create Account" />
      <Label fx:id="errorLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="4.0" layoutY="120.0" prefHeight="17.0" prefWidth="274.0" text="Please Log-in Above" textAlignment="CENTER" />
      <Label layoutX="26.0" layoutY="27.0" text="Welcome to the Course Review Application" />
      <Button layoutX="205.0" layoutY="145.0" mnemonicParsing="false" onAction="#handleCloseButton" prefHeight="25.0" prefWidth="50.0" text="Close" />
   </children>
</AnchorPane>
